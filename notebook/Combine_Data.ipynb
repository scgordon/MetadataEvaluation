   "source": [
    "tupleList=[w1.value,w2.value,w3.value]\n",
    "metadataEvaluation.CollectionComparisons=max(tupleList,key=len)\n",
    "metadataEvaluation.DataDestination=os.path.join('../data/Combined', str(w4.value)+'.csv')"
   ]
